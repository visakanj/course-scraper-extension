{
  "manifest_version": 3,
  "name": "Thinkific Course Scraper",
  "version": "2.0",
  "description": "Extract course content from Thinkific for migration and backup",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "downloads"
  ],
  "host_permissions": [
    "https://*.thinkific.com/*",
    "https://*.amazonaws.com/*"
  ]
}